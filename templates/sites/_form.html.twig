{{ form_start(form) }}

{{ dump(form) }}

<table>
    <tbody>
    <tr>
        <td>Name</td>
        <td>Firstname</td>
        <td>Adress</td>
    </tr>
    <tr>

        {% for userField in form.users %}
            <td>
                userField.name
                userField.firstName
                userField.adress
            </td>
        {% endfor %}
    </tr>
    </tbody>
</table>

    <button class="btn">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
