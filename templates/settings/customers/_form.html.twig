    <div class="content-import-enseigne">

        <div class="">
            <div class="row">
                <label class="form-label " for="customer_name">Nom de l'enseigne</label>
                {{ form_widget(form.name, {'attr': {'class': 'input-custome', ' placeholder' : 'Infoway',}}) }}
            </div>
            <div>
                <label class="form-label " >Importer logo</label>
                {{ form_widget(form.logo) }}
            </div>
        </div>
        <div>
        </div>
    </div>

    <div class="">
        {% for contact in form.contact  %}  
            {# {{dump(contact)}} #}
            {{ form_row(contact, {'attr': {'class': 'input-custome', ' placeholder' : 'Infoway',}}) }}
                                        
        {% endfor %}
    
        {# {{ form_widget(form) }} #}
    </div>
    
    <button class="btn">{{ button_label|default('Save') }}</button>
